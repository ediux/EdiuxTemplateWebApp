@model bool
@{
    var dropdownlistitems = new List<SelectListItem>();
    //if (ViewData.TemplateInfo.FormattedModelValue == null)
    //{
    //    ViewData.TemplateInfo.FormattedModelValue = false;
    //}
    ViewData[ViewData.TemplateInfo.HtmlFieldPrefix] = Model;
    bool flag;
    if (bool.TryParse(ViewData.TemplateInfo.FormattedModelValue as string, out flag))
    {
        flag = false;
    }
    dropdownlistitems.Add(new SelectListItem() { Text = "啟用", Value = "false", Selected = (flag == false) });
    dropdownlistitems.Add(new SelectListItem() { Text = "停用", Value = "true", Selected = (flag == true) });
}
@Html.DropDownList("",
    dropdownlistitems, new { @class = "form-control" })
